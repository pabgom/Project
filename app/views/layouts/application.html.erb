<!DOCTYPE html>
<html>
<head>
  <title>Project</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %> 
	<!-- <link href="/css/bootstrap.min" rel="stylesheet" /> -->

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<!-- Fixed navbar -->
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">BUYPROD</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/">Home</a></li>
            <li><a href="/search">Search</a></li>
            <li><a href="#contact">Contact</a></li>
						<% if user_signed_in? %>
						<li><%= link_to  'Orders', '/my_orders' %></li>
						<li><%= link_to  current_user.email , '/myprofile' %></li>
						<li><%= link_to 'Sign out', destroy_user_session_path, :method => :delete %></li>
							<% else %>
 						<li><%= link_to 'Register', new_user_registration_path %> </li>
						<li><%= link_to 'Sign in', new_user_session_path %></li>
						<% end %>
						
						<% if session["cart"] != nil and session["cart"] != {} and session["cart"] != '' %>						
						<li style="margin-top: 7px;" onclick="document.location.href='/cart'">
							<img src='/images/cart.png' alt="" widt="35px" height="35px" />
						</li>
						<% end %>
						
						<!--
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Dropdown <span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">
                <li><a href="#">Action</a></li>
                <li><a href="#">Another action</a></li>
                <li><a href="#">Something else here</a></li>
                <li class="divider"></li>
                <li class="dropdown-header">Nav header</li>
                <li><a href="#">Separated link</a></li>
                <li><a href="#">One more separated link</a></li>
              </ul>
            </li> -->
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>
<% if notice %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-danger"><%= alert %></p>
<% end %>
	
	<!-- Begin page content -->
    <div class="container container-fluid">
      <!-- <div class="page-header">
        <h1>Titulo</h1>
      </div>    -->
			<%= yield %>
    </div>

	
	


	
	<footer class="footer">
      <div class="container">
        <p class="text-muted">Place sticky footer content here.</p>
      </div>
    </footer>
</body>
</html>
